{
	"env": {
		"es2020": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"no-tabs": 0
	}
}
